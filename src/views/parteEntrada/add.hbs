<script>
    $(document).ready(function () {
        $("#myInput").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#myList li").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>
<div class="container p-4">
    <div class="row">
        <div class="col-md-4 mx-auto">
            <!--esto lo centra-->
            <div class="card">
                <div class="card-b/ody">
                    <p>Parte entrada</p>
                    <form action="/parteEntrada/add" method="POST">
                        <div class="form-group">
                            <input type="text" class="form-control" name="name" placeholder="cod" autofocus>
                        </div>
                        <h3>Ingresar articulos</h3>
                        <input class="form-control" id="myInput" type="text" placeholder="Search..">
                        <br>
                        <ul class="list-group" id="myList">
                            <li class="list-group-item">First item</li>
                            <li class="list-group-item">Second item</li>
                            <li class="list-group-item">Third item</li>
                            <li class="list-group-item">Fourth</li>
                        </ul>
                    

                </div>
            </div>
            <input class="d-none" name="example_two" readonly="readonly" aria-hidden="true" type="text" />
        </div>
        <div class="form-group">
            <input type="text" class="form-control" name="cant" placeholder="cantidad" autofocus>
        </div>

        <div class="form-group">
            <button class="btn btn-success btn-block">
                Guardar
            </button>
        </div>
        </form>
    </div>
</div>
</div>
